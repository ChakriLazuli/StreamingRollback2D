[mono]

project/assembly_name="StreamingRollback2D"

[network]

rollback/max_buffer_size=30
rollback/spawn_manager/reuse_despawned_nodes=true
rollback/sound_manager/default_sound_bus="Sound"
rollback/debug/rollback_ticks=5
rollback/debug/random_rollback_ticks=20

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
